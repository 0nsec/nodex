<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
	<cloud-backup disableIfNoEncryptionCapabilities="true">

		<exclude
			domain="root"
			path="." />

		<exclude
			domain="root"
			path="app_db" />
		<exclude
			domain="root"
			path="app_key" />

		<exclude
			domain="sharedpref"
			path="." />
	</cloud-backup>
	<device-transfer>

		<exclude
			domain="root"
			path="." />

		<exclude
			domain="root"
			path="app_db" />
		<exclude
			domain="root"
			path="app_key" />

		<exclude
			domain="sharedpref"
			path="." />
	</device-transfer>
</data-extraction-rules>
